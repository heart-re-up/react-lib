{
    "name": "@heart-re-up/react-lib-demo",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host --mode dev",
        "build": "pnpm build:prd",
        "build:dev": "tsc -b && vite build --mode dev",
        "build:uat": "tsc -b && vite build --mode uat",
        "build:prd": "tsc -b && vite build --mode prd",
        "build:analyze": "tsc -b && vite build --mode=analyze",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "format": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
        "format:fix": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
        "typecheck": "tsc --noEmit",
        "preview": "vite preview",
        "clean": "rimraf .turbo dist node_modules"
    },
    "dependencies": {
        "@heart-re-up/history-manager": "workspace:*",
        "@heart-re-up/history-manager-react": "workspace:*",
        "@heart-re-up/react-lib": "workspace:*",
        "@radix-ui/react-accordion": "^1.2.2",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-switch": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/themes": "^3.2.1",
        "@react-spring/web": "^10.0.1",
        "@tanstack/react-query": "^5.85.5",
        "clsx": "^2.1.1",
        "lodash-es": "^4.17.21",
        "lucide-react": "^0.540.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.62.0",
        "react-router": "^7.8.1",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@heart-re-up/eslint-config": "workspace:*",
        "@heart-re-up/prettier-config": "workspace:*",
        "@heart-re-up/typescript-config": "workspace:*",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.18.1",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.5.1",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.5.1",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.16",
        "typescript": "~5.8.3",
        "vite": "^7.1.3"
    }
}